<script>
    jQuery(document).ready(function() {
      
        $( ".iphone-6-6s label" ).text( "6" );              
        $( ".iphone-6-plus-6s-plus label" ).text( "6 Plus" );
        $( ".iphone-7 label" ).text( "7" );
        $( ".iphone-7-plus label" ).text( "7 Plus" );
        $( ".iphone-8 label" ).text( "8" );
        $( ".iphone-8-plus label" ).text( "8 Plus" );
        $( ".iphone-x label" ).text( "X" );

        $(".swatch-element input:radio").on('click', function(){
            if ($(this).attr('checked', 'checked')){
               $(this).parents(".swatch-element").addClass('active-swatch');
                $(this).parents(".swatch-element").siblings().removeClass('active-swatch');
                $(this).parents(".swatch").addClass('active-variant');
                $(this).parents(".swatch").siblings().removeClass('active-variant');
                $(this).parents(".product").addClass().addClass('active-product');
                $(this).parents(".product").siblings().removeClass('active-product');
                $(this).parents(".product").siblings('.product').children('.swatch').removeClass('active-variant');
                $(this).parents(".product").siblings('.product').children('.swatch-element').removeClass('checked');
            } else {
               $(this).parents(".swatch-element").removeClass('checked');
               $(this).parents(".swatch").removeClass('active-variant');
           }
        });
	});
</script>

<style>

{% assign width = '40px' %}
{% assign height = '40px' %}
.swatch .header {
  margin: 0.5em 0;
}
.swatch input { 
  display:none;
}
.swatch label {
  float:left;
  min-width:{{ width }} !important; 
  height:{{ height }} !important;
  margin:0;
  font-size:14px;
  text-align:center;
  line-height:{{ height }};
  white-space:nowrap;
  text-transform:uppercase;
  color: #343339;
}
.swatch-element label { padding:0 10px; }
.color.swatch-element label { padding:0;  border-radius: 0; }
.swatch .swatch-element {
  float:left;
  -webkit-transform:translateZ(0); /* webkit flicker fix */
  -webkit-font-smoothing:antialiased; /* webkit text rendering fix */
  margin:0px 10px 10px 0;
  position:relative;
}
/* Image with the cross in it */
.crossed-out { position:absolute; width:100%; height:100%; left:0; top:0; }
.swatch .swatch-element .crossed-out { display:none; }
.swatch .swatch-element.soldout .crossed-out { display:block; }
.swatch .swatch-element.soldout label {
  filter: alpha(opacity=60); /* internet explorer */
  -khtml-opacity: 0.6;      /* khtml, old safari */
  -moz-opacity: 0.6;       /* mozilla, netscape */
  opacity: 0.6;           /* fx, safari, opera */
}
/* Tooltips */
.swatch .tooltip {
  text-align:center;
  background:#343339;
  color:#fff;
  bottom:100%;
  padding: 10px;
  display:block;
  position:absolute;
  width:100px;
  left:{{ width | remove: 'px' | to_number | divided_by: 2 | minus: 50 | plus: 2 }}px;
  margin-bottom:15px;
  /* Make it invisible by default */
  filter:alpha(opacity=0);
  -khtml-opacity: 0;
  -moz-opacity: 0;
  opacity:0;
  visibility:hidden;
  /* Animations */
  -webkit-transform: translateY(10px);
     -moz-transform: translateY(10px);
      -ms-transform: translateY(10px);
       -o-transform: translateY(10px);
          transform: translateY(10px);
  -webkit-transition: all .25s ease-out;
     -moz-transition: all .25s ease-out;
      -ms-transition: all .25s ease-out;
       -o-transition: all .25s ease-out;
          transition: all .25s ease-out;
  -webkit-box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.28);
     -moz-box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.28);
      -ms-box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.28);
       -o-box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.28);
          box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.28);
  z-index: 10000;
  -moz-box-sizing:border-box; 
  -webkit-box-sizing:border-box; 
  box-sizing:border-box;
}
.swatch .tooltip:before {
  bottom:-20px;
  content:" ";
  display:block;
  height:20px;
  left:0;
  position:absolute;
  width:100%;
}
/* CSS triangle */
.swatch .tooltip:after {
  border-left:solid transparent 10px;
  border-right:solid transparent 10px;
  border-top:solid #343339 10px;
  bottom:-10px;
  content:" ";
  height:0;
  left:50%;
  margin-left:-13px;
  position:absolute;
  width:0;
}
.swatch .swatch-element:hover .tooltip {
  filter:alpha(opacity=100);
  -khtml-opacity:1;
  -moz-opacity:1;
  opacity:1;
  visibility:visible;
  -webkit-transform:translateY(0px);
     -moz-transform:translateY(0px);
      -ms-transform:translateY(0px);
       -o-transform:translateY(0px);
          transform:translateY(0px);
}
.swatch.error {
  background-color:#E8D2D2!important;
  color:#333!important;
  padding:1em;
  border-radius:5px;
}
.swatch.error p {
  margin:0.7em 0;
}
.swatch.error p:first-child {
  margin-top:0;
}
.swatch.error p:last-child {
  margin-bottom:0;
}
.swatch.error code {
  font-family:monospace;
}
</style>