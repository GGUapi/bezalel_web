                            <input type="number" id="Quantity" name="quantity" value="1" min="1" class="qty-remove-defaults quantity-selector" style="color:#000!important;" >
                        </div>   
                        <button type="submit" name="add" id="AddToCart" class="btn">
                            <span id="AddToCartText">現貨搶購</span><!--下方js $addToCartText-->
