      mutations.forEach(function(mutation) {
          if (mutation.attributeName === "class") {
              let classList = mutation.target.classList;
              if (classList.contains('opened')) {
                console.log("MutationObserver open");
                  // 如果'opened' class存在，就改變element2的背景顏色
                  element2.style.backgroundImage = 'url("https://cdn.shopify.com/s/files/1/1403/9023/files/new_xX.png?v=1690879553")';  // B
