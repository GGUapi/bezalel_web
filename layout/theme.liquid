                  element2.style.backgroundImage = 'url("https://cdn.shopify.com/s/files/1/1403/9023/files/4781852_burger_line_list_menu_nav_icon_0fec47ff-3ca1-47df-aac6-683b30df8c93.png?v=1691308437")';  // A
              }
          }
      });
  });

  // 配置觀察器：只觀察'element1'的class變化
  let config = { attributes: true, attributeFilter: ['class'] };

  // 開始觀察'element1'
  observer.observe(element1, config);



